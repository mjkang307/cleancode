#before
def getThem():
    result = []
    for x in theList:
        if x[0] == 4:
            result.append(x)
    return result

#after
def getFlaggedCells():
    flagged_cells = []
    for cell in game_board:
        if cell.status == FLAGGED:
            flagged_cells.append(cell)
    return flagged_cells
